{
  "name": "lesser",
  "description": "A minimalist workflow for designing responsive pages with Bootstrap and Less",
  "version": "0.1.1",
  "keywords": [
    "css",
    "less",
    "bootstrap",
    "workflow",
    "mobile-first",
    "responsive",
    "design",
    "typography"
  ],
  "main": [
    "src/js/design.js",
    "src/less/bootstrap.less",
    "src/less/colors.less",
    "src/less/layout.less",
    "src/less/typography.less",
    "gulpfile.js",
    "package.json"
  ],
  "ignore": [
    "/.*",
    "node_modules",
    "package.json",
    "public",
    ".jshintrc",
    "**/*.sh",
    "**/*.txt",
    "**/*.md"
  ],
  "dependencies": {
    "bootstrap": "~3.2.0",
    "letteringjs": "0.6.1"
  }
}
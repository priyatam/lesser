{
  "name": "lesser",
  "description": "A minimalist workflow for designing responsive pages with Bootstrap and Less",
  "version": "0.1.3",
  "keywords": [
    "less",
    "bootstrap",
    "workflow",
    "mobile-first",
    "responsive",
    "design",
    "typography"
  ],
  "main": [
    "src/js/typography.js",
    "src/js/layout.js",
    "src/less/bootstrap.less",
    "src/less/colors.less",
    "src/less/typography.less",
    "src/less/layout.less",
    "public",
    "gulpfile.js",
    "package.json"
  ],
  "ignore": [
    "/.*",
    "node_modules",
    "examples",
    "public/css/styles.min.css",
    "public/js/scripts.min.js",
    "public/img/*.*",
    ".jshintrc",
    "**/*.sh",
    "**/*.txt",
    "**/*.md"
  ],
  "dependencies": {
    "bootstrap": "~3.2.0",
    "FitText.js": "~1.2.0",
    "letteringjs": "0.6.1"
  }
}
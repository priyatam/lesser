{
  "name": "lesser",
  "description": "A minimalist workflow for designing responsive pages with Less",
  "version": "0.3.0",
  "keywords": [
    "less",
    "workflow",
    "responsive",
    "grids",
    "typography"
  ],
  "main": [
    "src/js/typography.js",
    "src/js/grids.js",
    "src/less/bootstrap.less",
    "src/less/colors.less",
    "src/less/typography.less",
    "src/less/grids.less"
  ],
  "ignore": [
    "/.*",
    "node_modules",
    "bower_components",
    "example",
    ".jshintrc",
    "**/*.txt",
    "**/*.md"
  ],
  "dependencies": {
    "bootstrap": "~3.3.0",
    "FitText.js": "~1.2.0",
    "letteringjs": "0.6.1",
    "Flowtype.js": "~1.1.0"
  }
}

{
    "name": "haiper",
    "version": "0.1.2",
    "description": "Haiku on Paper",
    "author": "Priyatam Mudivarti",
    "repository": {
        "type": "git",
        "url": "git://github.com/priyatam/haiper.git"
    },
    "bugs": {
        "url": "https://github.com/priyatam/haiper/issues"
    },
    "keywords": [
        "haiku",
        "css3",
        "rwd",
        "typography"
    ],
    "browserify": {
        "transform": [
            "browserify-shim"
        ]
    },
    "browserify-shim": {
        "plugin": {
            "exports": "plugin",
            "depends": [
                "jquery:$"
            ]
        }
    },
    "main": "gulpfile",
    "devDependencies": {
        "browserify": "~3.44.2",
        "gulp": "3.6.1",
        "gulp-jade": "0.5.0",
        "gulp-stylus": "~1.0.1",
        "gulp-marked": "0.2.0",
        "gulp-front-matter": "1.0.0",
        "gulp-connect": "~2.0.5",
        "gulp-buffer": "0.0.2",
        "gulp-cache": "0.1.3",
        "gulp-changed": "~0.3.0",
        "gulp-clean": "0.2.4",
        "gulp-concat": "2.2.0",
        "gulp-imagemin": "~0.1.5",
        "gulp-jshint": "1.5.3",
        "gulp-minify-css": "~0.3.4",
        "gulp-notify": "~1.2.4",
        "gulp-open": "~0.2.8",
        "gulp-print": "^1.0.3",
        "gulp-rename": "1.2.0",
        "gulp-svgmin": "~0.4.5",
        "gulp-uglify": "0.2.1",
        "gulp-replace-task": "~0.0.5",
        "gulp-util": "^2.2.14",
        "vinyl-source-stream": "~0.1.1"
    },
    "dependencies": {
        "jquery": "~2.1.0",
        "underscore": "~1.6.0"
    }
}
